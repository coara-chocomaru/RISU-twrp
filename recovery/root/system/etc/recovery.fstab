# Dynamic partitions
/system				    ext4		/dev/block/dm-2              flags=backup=1;display="System"
/product        	ext4		/dev/block/dm-1              flags=backup=1;display="Product"
/vendor				    ext4		/dev/block/dm-0              flags=backup=1;display="Vendor"
/product_image	  emmc		/dev/block/mapper/product	   flags=backup=1;display="Product Image";flashimg=1
/super          	emmc    /dev/block/by-name/super     flags=backup=1;display="Super_image";flashimg=1
/system_image	    emmc		/dev/block/mapper/system    flags=backup=1;display="System Image";flashimg=1
/vendor_image	    emmc		/dev/block/mapper/vendor    flags=backup=1;display="Vendor Image";flashimg=1
